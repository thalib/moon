{
	"docURL": "http://localhost:6006",
	"serverURL":  "https://moon.asensar.in",
	"prefix": "",
	"username": "admin",
	"password": "moonadmin12#",
	"health": "/health",
	"tests": [
		{
			"name": "",
			"cmd": "POST",
			"endpoint": "/users:create",
			"headers": {
				"Authorization": "Bearer $ACCESS_TOKEN",
				"Content-Type": "application/json"
			},
			"data": {
				"username": "newuser",
				"email": "newuser@example.com",
				"password": "UserPass123#",
				"role": "user"
			}
		},
		{
			"name": "Login",
			"cmd": "POST",
			"endpoint": "/auth:login",
			"headers": {
				"Content-Type": "application/json"
			},
			"data": {
				"username": "newuser",
				"password": "UserPass123#"
			}
		},
		{
			"name": "Refresh Token",
			"cmd": "POST",
			"endpoint": "/auth:refresh",
			"headers": {
				"Content-Type": "application/json"
			},
			"data": {
				"refresh_token": "$REFRESH_TOKEN"
			}
		},
		{
			"name": "Get Current User",
			"cmd": "GET",
			"endpoint": "/auth:me",
			"headers": {
				"Authorization": "Bearer $ACCESS_TOKEN"
			}
		},
		{
			"name": "Update Current User (Change email)",
			"cmd": "POST",
			"endpoint": "/auth:me",
			"headers": {
				"Authorization": "Bearer $ACCESS_TOKEN",
				"Content-Type": "application/json"
			},
			"data": {
				"email": "newemail@example.com"
			}
		},
		{
			"name": "Update Current User (Change Password)",
			"cmd": "POST",
			"endpoint": "/auth:me",
			"headers": {
				"Authorization": "Bearer $ACCESS_TOKEN",
				"Content-Type": "application/json"
			},
			"data": {
				"old_password": "UserPass123#",
				"password": "NewSecurePass456"
			}
		},
		{
			"name": "Logout",
			"cmd": "POST",
			"endpoint": "/auth:logout",
			"headers": {
				"Authorization": "Bearer $ACCESS_TOKEN",
				"Content-Type": "application/json"
			},
			"data": {
				"refresh_token": "$REFRESH_TOKEN"
			}
		}
	]
}
